<div id="page_login" class="full-heigh">
    <div class="container">
        <div class="input-session">
            <input id="input_username" class="margin-bottom input-input" maxlength="20" autocomplete="off" placeholder="Input your name" />
            <button class="btn btn-outline-secondary btn-lg" onclick="enterRoom()">Enter</button>
        </div>
    </div>
</div>

<div id="page_estimate" style="display: block;">
    <div class="container">
        <header class="padding-top">
            <div class="display-flex-element">
                <h1 style="flex: auto;">Time to Estimate Point</h1>
                <button id="reveadBtn" type="button" class="margin-top btn btn-outline-secondary btn-lg margin-right" data-toggle="modal" data-target="#myModal">Reveal Point</button>
                <button type="password" class="margin-top btn btn-outline-secondary btn-lg" onclick="reset()">Reset Board</button>

                <!-- Modal -->
                <div class="modal fade" id="myModal" role="dialog">
                    <div class="modal-dialog">
                        <!-- Modal content-->
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                                <h4 class="modal-title">Access to Reveal Point</h4>
                            </div>
                            <div class="modal-body">
                                <p>Fighting</p>
                                <input id="input_admin" class="margin-bottom admin-modal" maxlength="20" autocomplete="off" />

                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-default" data-dismiss="modal" onclick="reveal()">Access</button>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </header>
    </div>

    <section>

        <div class="container">
            <div class="input-session">
                <input id="input_point" class="set-center margin-bottom input-input" maxlength="10" autocomplete="off" />
                <button id="pointBtn" class="set-center btn btn-outline-secondary btn-lg" onclick="submit_answer()">Send Answer</button>
            </div>
        </div>


    </section>

    <section id="team" class="pb-5">
        <div class="container">
            <h5 class="section-title h1">TEAM MEMBER</h5>
            <div class="row">
                <div id="members_element" class="col-xs-12 col-sm-6 col-md-4" style="display: none;">
                    <div class="frontside">
                        <p class="card-addon"><img src="https://sunlimetech.com/portfolio/boot4menu/assets/imgs/team/img_01.png"></p>
                        <div class="card">
                            <div class="card-body text-center">
                                <h4>Sunlimetech</h4>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="members" class="row"></div>


            </div>
        </div>
    </section>

    <section style="padding-top: 0;">
        <!-- <div class="container"> -->
